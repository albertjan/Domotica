<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <httpModules>
      <add name="Gate" type="Gate.AspNet.Module, Gate.AspNet" />
    </httpModules>
  </system.web>
  <system.webServer>
