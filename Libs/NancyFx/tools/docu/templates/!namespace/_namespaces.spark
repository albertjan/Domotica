<div id="namespaces">
	<h2 class="fixed">Namespaces</h2>
	<div class="scroll">
		<ul>
			<li each="var ns in Namespaces">
        <if condition="ns == Namespace">
          <a href="../${ns.Name}/index.htm" class="current">${ns.Name}</a>
          <else />
          <a href="../${ns.Name}/index.htm">${ns.Name}</a>
        </if>
      </li>
		</ul>
	</div>
</div>